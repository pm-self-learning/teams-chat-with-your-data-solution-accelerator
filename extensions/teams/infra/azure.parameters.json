{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "resourceBaseName": {
      "value": "bot${{RESOURCE_SUFFIX}}"
    },
    "botAadAppClientId": {
      "value": "${{BOT_ID}}"
    },
    "botAadAppClientSecret": {
      "value": "${{SECRET_BOT_PASSWORD}}"
    },
    "azureFunctionURL": {
      "value": "${{AZURE_FUNCTION_URL}}"
    },
    "webAppSKU": {
      "value": "B1"
    },
    "botDisplayName": {
      "value": "teams-bot-toolkit"
    },
    "cosmosDBContainerName": {
      "value": "state"
    },
    "cosmosDBDatabaseName": {
      "value": "db_conversation_history"
    },
    "cosmosDBEndpoint": {
      "value": "https://jjtestcos.documents.azure.com:443/"
    },
    "cosmosDBKEY": {
      "value": "@Microsoft.KeyVault(SecretUri=https://kv-abc.vault.azure.net/secrets/AZURE-COSMOSDB-ACCOUNT-KEY)"
    }
  }
}
